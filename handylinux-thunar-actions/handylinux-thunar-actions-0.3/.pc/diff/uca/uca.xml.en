<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open a terminal here</name>
	<unique-id>1430681774901975-6</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>launch a terminal in the current directory</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>btshare</icon>
	<name>Share with BTshare</name>
	<unique-id>1430664757361524-1</unique-id>
	<command>/usr/bin/share_with_btshare %f</command>
	<description>share this file through P2P</description>
	<patterns>*</patterns>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>acroread</icon>
	<name>Convert in PDF</name>
	<unique-id>1430681789008349-7</unique-id>
	<command>unoconv -f pdf %f</command>
	<description>export this file in PDF in the current directory</description>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>browser</icon>
	<name>Send to ToileLibre</name>
	<unique-id>1430680843463192-5</unique-id>
	<command>/usr/bin/pixup %F</command>
	<description>upload this image to ToileLibre.org then share it</description>
	<patterns>*</patterns>
	<startup-notify/>
	<image-files/>
</action>
<action>
	<icon>catfish</icon>
	<name>Search</name>
	<unique-id>1430701366296475-16</unique-id>
	<command>/usr/bin/catfish --path=%f</command>
	<description>launch Catfish search-tool in the current directory</description>
	<patterns>*</patterns>
